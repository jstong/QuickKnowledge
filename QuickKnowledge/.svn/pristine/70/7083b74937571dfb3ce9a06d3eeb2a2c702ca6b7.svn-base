package com.quick.service.impl;

import com.quick.dao.ExpertRegisterDao;

import com.quick.entity.Expert;

import com.quick.entity.User;

import com.quick.service.ExpertRegisterService;

public class ExpertRegisterServiceImpl implements ExpertRegisterService {

	private ExpertRegisterDao expertregisterDao;

	public ExpertRegisterDao getExpertregisterDao() {
		return expertregisterDao;
	}

	public void setExpertregisterDao(ExpertRegisterDao ExpertregisterDao) {
		expertregisterDao = ExpertregisterDao;
	}

	public boolean addUser(User user, Expert expert) {
		// TODO Auto-generated method stub
		return expertregisterDao.addUser(user, expert);
	}

}
